<Window x:Class="wsr.inventItem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wsr"
        mc:Ignorable="d"
        Title="inventItem" Height="450" Width="800">
    <Grid Background="#FFE0A9AF">
        <DataGrid x:Name="itemlist" HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" VerticalAlignment="Top" Width="538" Loaded="DataGrid_Loaded"/>
        <Button x:Name="OK" Content="Утвердить" HorizontalAlignment="Left" Height="86" Margin="568,233,0,0" VerticalAlignment="Top" Width="216" Click="OK_Click"/>
        <Button Content="Выход" x:Name="exit" HorizontalAlignment="Left" Height="86" Margin="568,324,0,0" VerticalAlignment="Top" Width="216" Click="Exit_Click"/>

    </Grid>
</Window>
